{
  "name": "resorcery",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "main": "dist/resorcery.umd.js",
  "module": "dist/resorcery.mjs",
  "typings": "dist/types/resorcery.d.ts",
  "files": [
    "dist"
  ],
  "author": "Justin Ridgewell <jridgewell@google.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jridgewell/resorcery.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint:ts": "tslint  --project tsconfig.json --fix -t codeFrame '{src,test}/**/*.ts'",
    "lint:prettier": "prettier --write '{src,test}/**/*.ts'",
    "lint": "npm run lint:ts && npm run lint:prettier",
    "prebuild": "rm -rf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "test": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "jest --coverage --watch",
    "test:lint": "npm run lint:ts && prettier --check '{src,test}/**/*.ts'",
    "test:prod": "npm run test:lint && npm run test -- --no-cache"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/node": "12.6.2",
    "jest": "24.8.0",
    "jest-config": "24.8.0",
    "prettier": "1.18.2",
    "rollup": "1.16.7",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-typescript2": "0.21.2",
    "source-map": "0.6.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "sourcemap-codec": "1.4.6"
  }
}
