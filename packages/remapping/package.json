    "node": ">=6.0.0"
  },
  "scripts": {
    "lint:ts": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "lint:prettier": "prettier --write 'src/**/*.ts'",
    "lint": "npm run lint:ts && npm run lint:prettier",
    "prebuild": "rm -rf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:lint": "npm run lint:ts && prettier --check 'src/**/*.ts'",
    "test:prod": "npm run test:lint && npm run test -- --no-cache"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
