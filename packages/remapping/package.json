    "prettier": "^1.14.3",
    "rollup": "^1.16.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.21.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.0",
    "tslint": "^5.11.0",
